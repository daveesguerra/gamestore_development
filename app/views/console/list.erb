<% @page_title = 'Console List' %>
<h2>Console List</h2>
<div id="count"><%= pluralize(@consoles.size, 'console') %> found</div> <%= link_to('Add Console', {:action => 'new'},{:class => 'add-new'}) %><br />
<table class="list">
  <tr>
    <th>Console</th>
    <th>Actions</th>
  </tr>
  <% for console in @consoles -%>
    <tr>
      <td><%= link_to(console.console, {:action => 'show', :id => console.id}) %></td>
      <td><%= link_to('Edit', {:action => 'edit', :id => console.id}) %> | <%= link_to('Delete', {:action => 'delete', :id => console.id}) %></td>
    </tr>
  <% end -%>
</table>



